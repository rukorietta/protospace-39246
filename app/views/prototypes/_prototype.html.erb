<div class="card">
  <%= image_tag rails_blob_path(prototype.image, disposition: "attachment", only_path: true), class: "card-img-top" %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", root_path, class: :card__user %>
  </div>
</div>