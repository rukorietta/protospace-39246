<div class="card">
  <%= link_to image_tag(rails_blob_path(prototype.image, disposition: "attachment", only_path: true), class: "card-img-top"), prototype %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype), class: :card__title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", prototype.user, class: :card__user %>
  </div>
</div>